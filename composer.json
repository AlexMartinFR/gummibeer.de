{
    "name": "gummibeer/cv-resume",
    "description": "My personal website including cv & resume.",
    "keywords": [],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "~2.2",
        "php-http/guzzle6-adapter": "^1.1",
        "knplabs/github-api": "^2.5",
        "nesbot/carbon": "^1.22",
        "predis/predis": "^1.1",
        "illuminate/redis": "^5.4",
        "cache/redis-adapter": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "spatie/packagist-api": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "prefer-stable": true
