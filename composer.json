{
    "name": "gummibeer/cv-resume",
    "description": "My personal website including cv & resume.",
    "keywords": [],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "ext-redis": "*",
        "cache/redis-adapter": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/redis": "^5.8",
        "knplabs/github-api": "^2.11",
        "laravel/lumen-framework": "5.8.*",
        "league/commonmark": "^1.1",
        "nesbot/carbon": "^2.22",
        "php-http/guzzle6-adapter": "^1.1",
        "predis/predis": "^1.1",
        "spatie/laravel-blade-x": "^2.2",
        "spatie/packagist-api": "^1.2",
        "spatie/schema-org": "^2.3",
        "vlucas/phpdotenv": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
